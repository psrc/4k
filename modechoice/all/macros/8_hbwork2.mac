~/ ----------------------------------------
3.21
~+|1|y|mf"eutd"|n
exp
(
ms"ascctd"
+
ms"trdivt"*(mf"pnrtm"+mf"ivtda")
+
ms"trdovt"*(mf"wlkda"+mf"iwtda"+mf"xfrda")
+
ms"trdcos"*(mf"dapct"+mf"farda")
)
~+|||y|1,%1%||1,%1%||2
q
