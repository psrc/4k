~/ ----------------------------------------
~/ --- Calculate Auto Cost
~/ ----------------------------------------
3.21
~+|1|y|mf"dabct"|n|(mf"dabds"*ms"autoop")+(mf"dabcs"-mf"dabtm")/ms"avot2v"+(md"daily"/2)|||n|2
~+|1|y|mf"dapct"|n|(mf"pnrds"*ms"autoop")+(mf"auxda"-mf"pnrtm")/ms"avot2v"+(md"pnrchg"/2)|||n|2
~+|1|y|mf"s2bct"|n|((mf"s2bds"*ms"autoop")+(mf"s2bcs"-mf"s2btm")/ms"avots2"+(md"daily"/2))/2|||n|2
~+|1|y|mf"s3bct"|n|((mf"s3bds"*ms"autoop")+(mf"s3bcs"-mf"s3btm")/ms"avots3"+(md"daily"/2))/3.5|||n|2
q
~/ ----------------------------------------
~/ --- Calculate Drive Alone Utility
~/ ----------------------------------------
3.21
~+|1|y|mf"euda"|n
exp
(
ms"autivt"*
mf"dabtm"
~#(mf"dabtm"-mf"termtm")
~#+
~#ms"autovt"*
~#mf"termtm"
+
ms"autcos"*
mf"dabct"
)
~+|||y|1,%3%||1,%3%||2
q
