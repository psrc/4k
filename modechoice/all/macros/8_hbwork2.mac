~/ ----------------------------------------
3.21
~+|1|y|mf"eus2"|n
exp
(
ms"asccs2"
+
ms"autivt"*
mf"s2btm"
~#(mf"s2btm"-mf"termtm")
~#+
~#ms"autovt"*
~#mf"termtm"
+
ms"autcos"*
mf"s2bct"
)
~+|||y|1,%3%||1,%3%||2
q
